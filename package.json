{
  "name": "@ssv/signalr-client",
  "version": "1.0.0",
  "description": "SignalR client library built on top of @aspnet/signalr. This gives you more features and easier to use.",
  "homepage": "https://github.com/sketch7/signalr-client",
  "keywords": [
    "sketch7",
    "signalr",
    "signalr-client",
    "realtime",
    "push",
    "rxjs",
    "real-time",
    "angular",
    "aurelia",
    "vue",
    "react"
  ],
  "scripts": {
    "start": "microbundle watch",
    "build": "gulp build",
    "clean": "gulp clean",
    "lint": "gulp lint",
    "test": "jest",
    "tdd": "jest --watch",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --env jest-environment-node-debug",
    "test-devt": "devtool ./node_modules/jest/bin/jest.js --runInBand --env jest-environment-node-debug",
    "publish:rel": "npm publish dist --access=public"
  },
  "author": "Stephen Lautier <stephen.lautier@outlook.com>, Clayton Lautier <clayton.lautier@outlook.com>",
  "license": "MIT",
  "main": "dist/ssv.signalr-client.js",
  "umd:main": "dist/ssv.signalr-client.umd.js",
  "module": "dist/ssv.signalr-client.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/sketch7/signalr-client"
  },
  "bugs": {
    "url": "https://github.com/sketch7/signalr-client/issues"
  },
  "devDependencies": {
    "@ssv/tools": "^0.8.1",
    "@types/jest": "^23.3.13",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-bump": "^3.1.3",
    "gulp-conventional-changelog": "^2.0.9",
    "gulp-util": "^3.0.8",
    "jest": "^23.6.0",
    "jest-environment-node-debug": "^2.0.0",
    "microbundle": "^0.9.0",
    "require-dir": "^1.2.0",
    "rxjs": "^6.3.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4",
    "yargs": "^12.0.5"
  },
  "dependencies": {
    "@aspnet/signalr": "~1.1.0"
  },
  "peerDependencies": {
    "rxjs": "^6.2.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
